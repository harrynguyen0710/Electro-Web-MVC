@model WebDT.Models.TrangThaiDonHang

@{
    ViewData["Title"] = "Create";
    Layout = "_DashBoardAdmin";
}


<h4>Trạng thái đơn hàng</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="TenTrangThaiDonHang" class="control-label"></label>
                <input asp-for="TenTrangThaiDonHang" class="form-control" />
                <span asp-validation-for="TenTrangThaiDonHang" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

